;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	franmoreno.local. root.franmoreno.local. (
			20221121	; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	franmoreno.local.
@	IN	MX 10	franmoreno.local.

@	IN	A	192.168.1.254
mail	IN	A	192.168.1.254
dns	IN	A	192.168.1.254
dns2	IN	A	192.168.1.253

pc1	IN	A	192.168.1.1
pc2	IN	A	192.168.1.2
pc3	IN	A	192.168.1.3
pc4	IN	A	192.168.1.4
www	IN	A	192.168.1.100
ftp	IN	CNAME	www
moodle	IN	CNAME	www

